@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix : <http://www.w3.org/2006/time#> .
@prefix tzont: <http://www.w3.org/2006/timezone#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<http://www.w3.org/2006/time>
    a owl:Ontology ;
    rdfs:comment """
      An OWL Ontology of Time (OWL-Time).
    """ ;
    rdfs:seeAlso <http://www.w3.org/TR/owl-time> .

:DateTimeDescription
    a owl:Class ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:cardinality "1"^^xsd:nonNegativeInteger ;
        owl:onProperty :unitType
    ], [
        a owl:Restriction ;
        owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
        owl:onProperty :year
    ], [
        a owl:Restriction ;
        owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
        owl:onProperty :month
    ], [
        a owl:Restriction ;
        owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
        owl:onProperty :week
    ], [
        a owl:Restriction ;
        owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
        owl:onProperty :day
    ], [
        a owl:Restriction ;
        owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
        owl:onProperty :dayOfWeek
    ], [
        a owl:Restriction ;
        owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
        owl:onProperty :dayOfYear
    ], [
        a owl:Restriction ;
        owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
        owl:onProperty :hour
    ], [
        a owl:Restriction ;
        owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
        owl:onProperty :minute
    ], [
        a owl:Restriction ;
        owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
        owl:onProperty :second
    ], [
        a owl:Restriction ;
        owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
        owl:onProperty :timeZone
    ] .

:DateTimeInterval
    a owl:Class ;
    rdfs:subClassOf :ProperInterval .

:DayOfWeek
    a owl:Class ;
    owl:oneOf (:Sunday
        :Monday
        :Tuesday
        :Wednesday
        :Thursday
        :Friday
        :Saturday
    ) .

:DurationDescription
    a owl:Class ;
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
        owl:onProperty :years
    ], [
        a owl:Restriction ;
        owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
        owl:onProperty :months
    ], [
        a owl:Restriction ;
        owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
        owl:onProperty :weeks
    ], [
        a owl:Restriction ;
        owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
        owl:onProperty :days
    ], [
        a owl:Restriction ;
        owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
        owl:onProperty :hours
    ], [
        a owl:Restriction ;
        owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
        owl:onProperty :minutes
    ], [
        a owl:Restriction ;
        owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
        owl:onProperty :seconds
    ] .

:Friday
    a :DayOfWeek .

:Instant
    a owl:Class ;
    rdfs:subClassOf :TemporalEntity .

:Interval
    a owl:Class ;
    rdfs:subClassOf :TemporalEntity .

:January
    a owl:Class ;
    rdfs:subClassOf :DateTimeDescription, [
        a owl:Restriction ;
        owl:hasValue :unitMonth ;
        owl:onProperty :unitType
    ] .

:Monday
    a :DayOfWeek .

:ProperInterval
    a owl:Class ;
    rdfs:subClassOf :Interval ;
    owl:disjointWith :Instant .

:Saturday
    a :DayOfWeek .

:Sunday
    a :DayOfWeek .

:TemporalEntity
    a owl:Class ;
    owl:unionOf (:Instant
        :Interval
    ) .

:TemporalUnit
    a owl:Class ;
    owl:oneOf (:unitSecond
        :unitMinute
        :unitHour
        :unitDay
        :unitWeek
        :unitMonth
        :unitYear
    ) .

:Thursday
    a :DayOfWeek .

:Tuesday
    a :DayOfWeek .

:Wednesday
    a :DayOfWeek .

:Year
    a owl:Class ;
    rdfs:subClassOf :DurationDescription, [
        a owl:Restriction ;
        owl:cardinality "1"^^xsd:nonNegativeInteger ;
        owl:onProperty :years
    ], [
        a owl:Restriction ;
        owl:cardinality "0"^^xsd:nonNegativeInteger ;
        owl:onProperty :months
    ], [
        a owl:Restriction ;
        owl:cardinality "0"^^xsd:nonNegativeInteger ;
        owl:onProperty :weeks
    ], [
        a owl:Restriction ;
        owl:cardinality "0"^^xsd:nonNegativeInteger ;
        owl:onProperty :days
    ], [
        a owl:Restriction ;
        owl:cardinality "0"^^xsd:nonNegativeInteger ;
        owl:onProperty :hours
    ], [
        a owl:Restriction ;
        owl:cardinality "0"^^xsd:nonNegativeInteger ;
        owl:onProperty :minutes
    ], [
        a owl:Restriction ;
        owl:cardinality "0"^^xsd:nonNegativeInteger ;
        owl:onProperty :seconds
    ] .

:after
    a owl:ObjectProperty ;
    owl:inverseOf :before .

:before
    a owl:ObjectProperty, owl:TransitiveProperty ;
    rdfs:domain :TemporalEntity ;
    rdfs:range :TemporalEntity .

:day
    a owl:DatatypeProperty ;
    rdfs:domain :DateTimeDescription ;
    rdfs:range xsd:gDay .

:dayOfWeek
    a owl:ObjectProperty ;
    rdfs:domain :DateTimeDescription ;
    rdfs:range :DayOfWeek .

:dayOfYear
    a owl:DatatypeProperty ;
    rdfs:domain :DateTimeDescription ;
    rdfs:range xsd:nonNegativeInteger .

:days
    a owl:DatatypeProperty ;
    rdfs:domain :DurationDescription ;
    rdfs:range xsd:decimal .

:hasBeginning
    a owl:ObjectProperty ;
    rdfs:domain :TemporalEntity ;
    rdfs:range :Instant .

:hasDurationDescription
    a owl:ObjectProperty ;
    rdfs:domain :TemporalEntity ;
    rdfs:range :DurationDescription .

:hasEnd
    a owl:ObjectProperty ;
    rdfs:domain :TemporalEntity ;
    rdfs:range :Instant .

:hour
    a owl:DatatypeProperty ;
    rdfs:domain :DateTimeDescription ;
    rdfs:range xsd:nonNegativeInteger .

:hours
    a owl:DatatypeProperty ;
    rdfs:domain :DurationDescription ;
    rdfs:range xsd:decimal .

:inside
    a owl:ObjectProperty ;
    rdfs:domain :Interval ;
    rdfs:range :Instant .

:intervalAfter
    a owl:ObjectProperty ;
    owl:inverseOf :intervalBefore .

:intervalBefore
    a owl:ObjectProperty ;
    rdfs:domain :ProperInterval ;
    rdfs:range :ProperInterval ;
    rdfs:subPropertyOf :before .

:intervalContains
    a owl:ObjectProperty ;
    owl:inverseOf :intervalDuring .

:intervalDuring
    a owl:ObjectProperty ;
    rdfs:domain :ProperInterval ;
    rdfs:range :ProperInterval .

:intervalEquals
    a owl:ObjectProperty ;
    rdfs:domain :ProperInterval ;
    rdfs:range :ProperInterval .

:intervalFinishedBy
    a owl:ObjectProperty ;
    owl:inverseOf :intervalFinishes .

:intervalFinishes
    a owl:ObjectProperty ;
    rdfs:domain :ProperInterval ;
    rdfs:range :ProperInterval .

:intervalMeets
    a owl:ObjectProperty ;
    rdfs:domain :ProperInterval ;
    rdfs:range :ProperInterval .

:intervalMetBy
    a owl:ObjectProperty ;
    owl:inverseOf :intervalMeets .

:intervalOverlappedBy
    a owl:ObjectProperty ;
    owl:inverseOf :intervalOverlaps .

:intervalOverlaps
    a owl:ObjectProperty ;
    rdfs:domain :ProperInterval ;
    rdfs:range :ProperInterval .

:intervalStartedBy
    a owl:ObjectProperty ;
    owl:inverseOf :intervalStarts .

:intervalStarts
    a owl:ObjectProperty ;
    rdfs:domain :ProperInterval ;
    rdfs:range :ProperInterval .

:minute
    a owl:DatatypeProperty ;
    rdfs:domain :DateTimeDescription ;
    rdfs:range xsd:nonNegativeInteger .

:minutes
    a owl:DatatypeProperty ;
    rdfs:domain :DurationDescription ;
    rdfs:range xsd:decimal .

:month
    a owl:DatatypeProperty ;
    rdfs:domain :DateTimeDescription ;
    rdfs:range xsd:gMonth .

:months
    a owl:DatatypeProperty ;
    rdfs:domain :DurationDescription ;
    rdfs:range xsd:decimal .

:second
    a owl:DatatypeProperty ;
    rdfs:domain :DateTimeDescription ;
    rdfs:range xsd:decimal .

:seconds
    a owl:DatatypeProperty ;
    rdfs:domain :DurationDescription ;
    rdfs:range xsd:decimal .

:timeZone
    a owl:ObjectProperty ;
    rdfs:domain :DateTimeDescription ;
    rdfs:range tzont:TimeZone .

:unitDay
    a :TemporalUnit .

:unitHour
    a :TemporalUnit .

:unitMinute
    a :TemporalUnit .

:unitMonth
    a :TemporalUnit .

:unitSecond
    a :TemporalUnit .

:unitType
    a owl:ObjectProperty ;
    rdfs:domain :DateTimeDescription ;
    rdfs:range :TemporalUnit .

:unitWeek
    a :TemporalUnit .

:unitYear
    a :TemporalUnit .

:week
    a owl:DatatypeProperty ;
    rdfs:domain :DateTimeDescription ;
    rdfs:range xsd:nonNegativeInteger .

:weeks
    a owl:DatatypeProperty ;
    rdfs:domain :DurationDescription ;
    rdfs:range xsd:decimal .

:year
    a owl:DatatypeProperty ;
    rdfs:domain :DateTimeDescription ;
    rdfs:range xsd:gYear .

:years
    a owl:DatatypeProperty ;
    rdfs:domain :DurationDescription ;
    rdfs:range xsd:decimal .

tzont:TimeZone
    a owl:Class .

